<?xml version="1.0" encoding="UTF-8"?>
<plugin name="lincebi" title="${lincebi}" loader="DEFAULT">

	<static-paths>
		<static-path url="/lincebi/resources" localFolder="resources" />
	</static-paths>

	<perspective id="lincebi.perspective" title="${lincebi}" layout-priority="1000"
		resourcebundle="content/lincebi/resources/messages/messages"
		content-url="content/lincebi/resources/html/index.html">
	</perspective>

	<content-generator id="url.generatedContent" type="url">
		<classname>com.stratebi.lincebi.urlaudit.UrlContentGenerator</classname>
		<title>URL Audit Content Generator</title>
	</content-generator>

	<lifecycle-listener class="com.stratebi.lincebi.lifecyclelistener.LifecycleListener" />

</plugin>
